; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "Config",
"language": "GDScript",
"path": "res://config.gd"
}, {
"base": "Node",
"class": "ErrorUtils",
"language": "GDScript",
"path": "res://utils/ErrorUtils.gd"
}, {
"base": "Node",
